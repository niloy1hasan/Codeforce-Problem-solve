#include <iostream>
using namespace std;

int main(){

    int t;
    cin >> t;

    while(t--){
        long long int l, r;
        cin >> l >> r;
        int count = 0;
        int dif = 1;
        for(int i=l; i<=r; i+=dif, dif++){
             count++;
        }

        cout << count << endl;
    }
    return 0;
}
